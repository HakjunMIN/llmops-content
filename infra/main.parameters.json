      "value": "${AZURE_KEY_VAULT_NAME}"
    },
    "logAnalyticsName": {
      "value": "${AZURE_LOG_ANALYTICS_NAME}"
    },
    "openAiName": {
      "value": "${AZURE_OPENAI_NAME}"
    },
    "searchServiceName": {
      "value": "${AZURE_SEARCH_NAME}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT_NAME}"
    },
    "azureSearchIndexSampleData": {
      "value": "${LOAD_AZURE_SEARCH_SAMPLE_DATA}"
    },
    "promptFlowWorkerNum": {
      "value": "${PROMPTFLOW_WORKER_NUM}"
    },
    "promptFlowServingEngine": {
      "value": "${PROMPTFLOW_SERVING_ENGINE}"
    }
  }
}